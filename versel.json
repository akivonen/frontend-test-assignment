{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/dist/images/$1"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/dist/fonts/$1"
    }
  ]
}
